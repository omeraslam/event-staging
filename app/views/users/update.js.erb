<% if !@user.profile_img.nil? %>
$('.avatar').css('background-image', 'url(<%= @user.profile_img %>)');
<% else %>
    alert('file image upload error') 
<% end %>

<% if !@user.header_img.nil? %>

$('.header-preview').attr('src', '<%= @user.header_img %>');
<% else %>
    alert('file image upload error')
<% end %>
